{
	"name": "@rediska1114/tiktok-api",
	"version": "1.0.1",
	"description": "TikTok API library for fetching user profiles, posts, and challenges",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./*": {
			"types": "./dist/*.d.ts",
			"import": "./dist/*.mjs",
			"require": "./dist/*.cjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"tiktok",
		"api",
		"scraper",
		"tiktok-api",
		"user-profile",
		"posts",
		"challenge"
	],
	"author": "",
	"license": "ISC",
	"type": "module",
	"peerDependencies": {
		"async-retry": "^1.0.0",
		"axios": "^1.0.0",
		"https-proxy-agent": "^7.0.0"
	},
	"devDependencies": {
		"@types/async-retry": "^1.4.8",
		"@types/node": "^24.7.0",
		"async-retry": "^1.3.3",
		"axios": "^1.12.2",
		"glob": "^11.0.3",
		"https-proxy-agent": "^7.0.6",
		"typescript": "^5.9.3",
		"vite": "^7.1.9",
		"vite-plugin-dts": "^4.5.4"
	}
}
